<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.pdm.klebson.lostboard">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainHome"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_perdi_minha_placa"
            android:label="@string/title_activity_activity_perdi_minha_placa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_encontrei_uma_placa"
            android:label="@string/title_activity_activity_encontrei_uma_placa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_lista_de_placa"
            android:label="@string/title_activity_activity__lista_de_placa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_teste_web_service"
            android:label="@string/title_activity_activity_teste_web_service"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
